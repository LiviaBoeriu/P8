@model IEnumerable<P8Project.Models.PlayerProfile>

@{
    ViewBag.Title = "FilterMatches";
}

<h2>FilterMatches</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ProfileTitle.title)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.first_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Location.zip_code)
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ProfileTitle.title)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.first_name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Location.zip_code)
            </td>
        </tr>
    }

</table>
